{
  "name": "@imuzik/mobile",
  "version": "0.0.1",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "precommit": "lint-staged",
    "start": "next start -p 8200",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "dev": "node server.js",
    "devnext": "next dev -p 3001",
    "eject": "expo eject",
    "build": "next build",
    "lint": "yarn eslint --ext .js,.jsx,.ts,.tsx src pages --color",
    "prestorybook": "rnstl",
    "storybook": "start-storybook",
    "build-storybook": "build-storybook",
    "test": "jest --watch --coverage=false --changedSince=master",
    "test:debug": "jest -o --watch --coverage=false",
    "test:ci": "jest && yarn test:type",
    "test:type": "tsc --project . --noEmit",
    "gen:gql": "graphql-codegen --config ../queries/codegen.yml"
  },
  "jest": {
    "preset": "jest-expo",
    "coverageReporters": [
      "text",
      "json-summary",
      "html"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/components/**/*.{ts,tsx}",
      "!src/components/Player/*Adapter.{ts,tsx}",
      "!src/error-codes.ts",
      "!src/queries.tsx",
      "!src/hooks/*",
      "!src/helpers/*",
      "!src/components/SvgIcon/types.ts",
      "!src/rebass/index.ts",
      "!src/**/index.ts",
      "!src/wap-containers/**/*",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ],
    "coverageThreshold": {
      "global": {
        "statements": 84.07,
        "branches": 73.35,
        "functions": 80,
        "lines": 85
      }
    },
    "transform": {
      "^.+\\.stories\\.js$": "@storybook/addon-storyshots/injectFileName"
    }
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "@apollo/react-common": "^3.1.4",
    "@apollo/react-hooks": "^3.1.5",
    "@apollo/react-ssr": "^3.1.5",
    "@expo/next-adapter": "^3.1.21",
    "@expo/react-native-action-sheet": "^3.6.0",
    "@react-native-async-storage/async-storage": "^1.17.3",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/bottom-tabs": "^5.2.7",
    "@react-navigation/native": "^5.9.4",
    "@react-navigation/stack": "^5.2.13",
    "@types/apollo-upload-client": "^17.0.0",
    "@types/react-helmet": "^5.0.15",
    "@types/react-native-base64": "^0.2.0",
    "@types/react-native-modal": "^4.1.1",
    "@types/react-native-swipe-gestures": "^1.0.4",
    "@types/styled-system": "^5.1.10",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link-http": "^1.5.16",
    "apollo-upload-client": "^17.0.0",
    "axios": "^0.27.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "buffer": "^6.0.3",
    "date-fns": "^2.12.0",
    "eas-cli": "^0.48.2",
    "expo": "^44.0.0",
    "expo-av": "~10.2.0",
    "expo-document-picker": "~10.1.0",
    "expo-file-system": "^13.1.4",
    "expo-image-manipulator": "~10.2.0",
    "expo-image-picker": "~12.0.1",
    "expo-linear-gradient": "~11.0.0",
    "expo-network": "~4.1.0",
    "expo-notifications": "~0.14.0",
    "expo-permissions": "~13.1.0",
    "expo-secure-store": "~11.1.0",
    "expo-sms": "~10.1.0",
    "expo-updates": "~0.11.6",
    "express": "^4.17.2",
    "global": "^4.4.0",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.0.0",
    "http-proxy-middleware": "^1.0.6",
    "isomorphic-unfetch": "^3.0.0",
    "modal-react-native-web": "^0.2.0",
    "ms": "^2.1.3",
    "next": "^10.0.9",
    "next-images": "^1.3.1",
    "prettier": "^2.0.4",
    "react": "17.0.1",
    "react-div-100vh": "^0.3.8",
    "react-dom": "17.0.1",
    "react-helmet": "^5.2.1",
    "react-native": "0.64.3",
    "react-native-appearance": "~0.3.3",
    "react-native-base64": "^0.2.1",
    "react-native-gesture-handler": "~2.1.0",
    "react-native-get-mp3-duration": "^1.0.3",
    "react-native-keyboard-aware-scroll-view": "^0.9.4",
    "react-native-mime-types": "^2.3.0",
    "react-native-modal": "^13.0.1",
    "react-native-reanimated": "^2.3.1",
    "react-native-render-html": "^6.3.0",
    "react-native-safe-area-context": "3.3.2",
    "react-native-screens": "~3.10.1",
    "react-native-svg": "12.1.1",
    "react-native-web": "0.17.1",
    "react-native-webview": "11.15.0",
    "react-native-xml2js": "^1.0.3",
    "react-spinners": "^0.11.0",
    "react-toast-notifications": "^2.4.4",
    "styled-components": "^5.0.1",
    "styled-system": "^5.1.5"
  },
  "devDependencies": {
    "@apollo/react-testing": "^3.1.4",
    "@babel/core": "^7.12.9",
    "@graphql-codegen/add": "^1.13.1",
    "@graphql-codegen/cli": "^1.21.6",
    "@graphql-codegen/fragment-matcher": "^1.13.3",
    "@graphql-codegen/introspection": "^1.13.1",
    "@graphql-codegen/typescript": "^1.13.1",
    "@graphql-codegen/typescript-operations": "^1.13.1",
    "@graphql-codegen/typescript-react-apollo": "^1.13.1",
    "@react-native-community/eslint-config": "^2.0.0",
    "@storybook/addon-actions": "^5.3.17",
    "@storybook/addon-info": "^5.3.17",
    "@storybook/addon-knobs": "^5.3.17",
    "@storybook/addon-links": "^5.3.17",
    "@storybook/addon-storyshots": "^5.3.17",
    "@storybook/addons": "^5.3.17",
    "@storybook/preset-typescript": "^2.1.0",
    "@storybook/react-native": "^5.3.17",
    "@storybook/react-native-server": "^5.3.17",
    "@testing-library/react-native": "^7.2.0",
    "@types/jest": "^25.1.4",
    "@types/lodash": "^4.14.168",
    "@types/ms": "^0.7.31",
    "@types/react": "~17.0.21",
    "@types/react-native": "~0.64.12",
    "@types/react-test-renderer": "^16.9.2",
    "@types/styled-components": "^5.0.1",
    "@types/styled-components-react-native": "^5.1.1",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "babel-loader": "8.0.6",
    "babel-preset-expo": "9.0.1",
    "eslint": "^7.17.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-module-resolver": "^1.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "fork-ts-checker-webpack-plugin": "^4.1.1",
    "jest-expo": "^44.0.0",
    "lint-staged": "^10.1.7",
    "prettier": "^2.0.4",
    "prettier-eslint": "^9.0.1",
    "react-docgen-typescript-loader": "^3.7.1",
    "react-native-storybook-loader": "^1.8.1",
    "react-test-renderer": "~16.9.0",
    "ts-loader": "^6.2.2",
    "typescript": "~4.3.5",
    "waait": "^1.0.5",
    "webpack-bundle-analyzer": "^3.7.0"
  },
  "private": true,
  "eslintConfig": {
    "plugins": [
      "prettier"
    ],
    "extends": "universe/native",
    "rules": {
      "prettier/prettier": "error",
      "flowtype/no-types-missing-file-annotation": 0
    }
  },
  "lint-staged": {
    "**/*.{ts,tsx,js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "**/*.json": [
      "prettier --write"
    ]
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./storybook"
      ],
      "pattern": "**/*.stories.js",
      "outputFile": "./storybook/storyLoader.js"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn precommit"
    }
  }
}
