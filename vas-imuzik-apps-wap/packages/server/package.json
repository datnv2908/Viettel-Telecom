    "rxjs": "^6.5.4",
    "sha1": "^1.1.1",
    "soap": "^0.31.0",
    "svg-captcha": "^1.4.0",
    "type-graphql": "^0.17.6",
    "typeorm": "^0.2.24",
    "uuid": "^7.0.2"
