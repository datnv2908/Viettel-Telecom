    "@nestjs/jwt": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/serve-static": "^2.1.0",
    "@nestjs/typeorm": "^7.0.0",
    "apollo-server-express": "^2.11.0",
    "body-parser": "^1.19.0",
    "cache-manager": "^3.2.1",
    "camelcase-keys": "^6.2.1",
    "chai": "^4.2.0",
    "class-validator": "^0.11.1",
    "cookie-parser": "^1.4.5",
    "dataloader": "^2.0.0",
    "date-fns": "^2.11.1",
    "date-fns-tz": "^1.0.10",
    "dotenv": "^10.0.0",
    "fp-ts": "^2.5.3",
    "graphql": "^14.6.0",
    "graphql-depth-limit": "^1.1.0",
    "graphql-relay": "^0.6.0",
    "graphql-tools": "^4.0.7",
