    "@types/lru-cache": "^5.1.0",
    "@types/mongodb": "^3.5.8",
    "@types/ms": "^0.7.31",
    "@types/node": "^13.9.1",
    "@types/sha1": "^1.1.2",
    "@types/sinon": "^9.0.0",
    "@types/soap": "^0.21.0",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^7.0.2",
    "@typescript-eslint/eslint-plugin": "^2.23.0",
    "@typescript-eslint/parser": "^2.23.0",
    "cucumber": "^6.0.5",
    "cucumber-pretty": "^6.0.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.3.1",
    "husky": "^6.0.0",
    "jest": "^25.1.0",
    "lint-staged": "^10.1.7",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.1",
    "prettier": "^2.0.4",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.1",
    "supertest": "^4.0.2",
    "ts-jest": "25.2.1",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "testEnvironment": "node",
    "coverageDirectory": "./../.nyc_output/",
    "coverageReporters": [
      "json"
    ]
  },
  "lint-staged": {
    "src/**/*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  },
  "nyc": {
    "reporter": []
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn precommit"
    }
  }
}
